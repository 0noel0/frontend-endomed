    <script type="text/javascript">
    $(function() {
        'use strict';

        // toggle important state
        $('.mc-group-item').on('click', function(e){
            e.preventDefault()

            $('.mc-group-item').removeClass('active');
            $(this).addClass('active');
            $('.message-preview').addClass('open');
        })

        // back to show mc-group, only for small phone viewport
        $('.back-to-mc-group').on('click', function(){
            $('.message-preview').removeClass('open');
        })
        
        // toggle important state
        $('.mcgi-star').on('click', function(e){
            e.preventDefault();

            $(this).toggleClass('important');
        })
        // end mark as important stat

        // message content checker
        $('.icheck').iCheck({
            checkboxClass: 'icheckbox_minimal-blue',
            radioClass: 'iradio_minimal-blue',
        });

        // checked all state
        $('[name="mcgi-check-all"]').on('ifChecked', function(){
            $('[name="mcgi-checkbox"]').iCheck('check');
        })
        .on('ifUnchecked', function(){
            $('[name="mcgi-checkbox"]').iCheck('uncheck');
        });

        // single checked state
        $('[name="mcgi-checkbox"]').on('ifToggled', function(e){
            var isAnyChecked = $('[name="mcgi-checkbox"]:checked').length,
                msg_item = $(this).parent().parent().parent();

            msg_item.toggleClass('selected');
                
            if (!isAnyChecked) {
                $('.mcgi-checker').removeClass('checked');
                $('[name="mcgi-check-all"]').iCheck('uncheck');
            }
            else{
                $('.mcgi-checker').addClass('checked');
            };
        });
        // end message content checker
    })
    </script>
  </body>
</html>